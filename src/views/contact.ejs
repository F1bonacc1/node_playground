<div class="form-header">
  <h2>Pull remote changes</h2>
</div>
<form method="post" action="/contact" novalidate>
  <div class="form-field">
    <label for="message">User</label>
    <textarea class="input" id="message" name="message" autofocus></textarea>
  </div>
  <div class="form-field">
    <label for="email">Password</label>
    <input class="input" id="email" name="email" type="password" value="" />
  </div>
  <div class="form-actions">
    <button class="btn" type="submit">Pull</button>
  </div>
</form>
